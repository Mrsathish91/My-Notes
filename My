"OU=YourOU,DC=YourDomain,DC=com"

-Properties * |

Select-Object @{Name='Name';Expression

={$_.Name}},

@{Name=

'Username';Expression={$

.SamAccountName}},

@{Name='Email';Expression

={$_.mail}},

@{Name=

'Department';Expression={$

.Department}} |

